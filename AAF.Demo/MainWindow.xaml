<dx:DXWindow x:Class="AAF.Demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
        xmlns:pages="clr-namespace:AAF.Demo.Pages"
        dx:ThemeManager.ThemeName="Office2013LightGray"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AAF.Demo"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Grid>
        <dx:DXTabControl x:Name="tabControl">
            <dx:DXTabItem Header="数据提取">

                <Grid  Background="Transparent">
                    <pages:PExtractor/>
                </Grid>
            </dx:DXTabItem>
            <dx:DXTabItem Header="本地搜索">
                <Grid  Background="Transparent">
                    <pages:PSearcher/>
                </Grid>
            </dx:DXTabItem>
            <dx:DXTabItem Header="设备搜索">
                <Grid  Background="Transparent">
                    <pages:PDeviceSearcher/>
                </Grid>
            </dx:DXTabItem>
        </dx:DXTabControl>
        
    </Grid>
</dx:DXWindow>
